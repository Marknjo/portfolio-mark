extends ../emailBaseTemplate

block content
  - let messageInfo = message.split("\n").filter(ms => ms !== " ")
  each ms in messageInfo
    p= ms
 
  tbody
  tr
    td(align='left')
      table(role='presentation', border='0', cellpadding='0', cellspacing='0')
        tbody
          tr
            td
              a(href=`mailto:${email}`, target='_blank') Mail Back #{name}
